{
  "name": "peg-game",
  "version": "1.0.0",
  "description": "Peg game with React, Redux and React Motion",
  "engines": {
    "node": "6.9.1"
  },
  "scripts": {
    "build": "npm run clean && npm run build:webpack",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js --progress",
    "clean": "rimraf dist",
    "dev": "cross-env NODE_ENV=development npm start",
    "lint": "eslint src",
    "start": "node server.js",
    "test": "karma start --single-run",
    "test:chrome": "karma start --single-run --browsers Chrome",
    "test:debug": "karma start --browsers=Chrome --single-run=false --debug",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RyanFuentes/Peg-Game.git"
  },
  "license": "MIT",
  "homepage": "https://github.com/RyanFuentes/Peg-Game",
  "dependencies": {
    "color-scheme": "0.0.5",
    "github-fork-ribbon-css": "^0.2.1",
    "radium": "^0.18.1",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-motion": "^0.4.7",
    "react-redux": "^4.1.1",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "redux-actions": "^1.0.0",
    "redux-logger": "^2.7.4",
    "redux-optimist": "0.0.2",
    "redux-simple-promise": "^2.0.2",
    "redux-slider-monitor": "^1.0.7",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.1",
    "babel-core": "^6.3.15",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.25.0",
    "enzyme": "^2.6.0",
    "eslint": "^3.8.1",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^6.4.1",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.13.3",
    "file-loader": "^0.9.0",
    "jasmine-core": "^2.5.2",
    "json-loader": "^0.5.4",
    "karma": "^1.3.0",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.8.0",
    "node-sass": "^3.12.2",
    "phantomjs-polyfill-find": "github:ptim/phantomjs-polyfill-find",
    "phantomjs-prebuilt": "^2.1.13",
    "postcss-loader": "^1.1.1",
    "react-addons-test-utils": "^15.3.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "redux-devtools": "^3.0.1",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.2",
    "redux-logger": "^2.4.0",
    "redux-mock-store": "^1.2.1",
    "rimraf": "^2.4.3",
    "sass-loader": "^4.0.2",
